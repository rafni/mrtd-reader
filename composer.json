{
    "name": "rafni/mrtd-reader",
    "type": "mrtd-reader",
    "description": "Reader for ICAO 9303 Machine Readable Travel Documents (MRTDs)",
    "license": "MIT",
    "require": {
        "php": "^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Rafni\\MrtdReader\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rafni\\MrtdReader\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
